{
  "name": "ember-appointment-slots-pickers",
  "version": "2.1.0",
  "description": "Appointment slot pickers component suite written in Ember.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "github:britishgas-engineering/ember-appointment-slots-pickers",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "broccoli-funnel": "3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "5.7.2",
    "ember-cli-less": "^3.0.2",
    "ember-component-css": "^0.6.9",
    "ember-gestures": "2.0.1",
    "ember-truth-helpers": "2.1.0",
    "pickadate": "3.6.4",
    "sly-shim": "1.6.1"
  },
  "devDependencies": {
    "@ember/jquery": "^0.6.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "bootstrap": "3.4.1",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "~3.28.5",
    "ember-cli-app-version": "3.0.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-moment-shim": "3.8.0",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~3.28.0",
    "ember-load-initializers": "2.1.2",
    "ember-moment": "7.8.1",
    "ember-page-title": "^6.2.2",
    "ember-resolver": "8.0.3",
    "ember-route-action-helper": "2.0.7",
    "ember-sinon": "5.0.0",
    "ember-source": "~3.28.8",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "font-awesome": "4.5.0",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
